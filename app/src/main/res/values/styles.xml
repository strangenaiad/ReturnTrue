    <style name="DragAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/thrill</item>
        <item name="android:windowShowAnimation">@anim/thrill</item>
        <item name="android:windowExitAnimation">@anim/popup_exit</item>
    </style>

    <style name="PopupWindowAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/thrill</item>
        <item name="android:windowExitAnimation">@anim/small</item>
    </style>

    <style name="PopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popup_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_exit</item>
        <!-- <item name="android:windowExitAnimation">@animator/shake</item> -->
    </style>

    <style name="DoWindowAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/in</item>
        <item name="android:windowExitAnimation">@anim/out</item>
        <!-- <item name="android:windowExitAnimation">@animator/shake</item> -->
    </style>

    <style name="phone_Animation.Dialog.BottomIn300" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/phone_i_anim_bottom_in_300</item>
        <item name="android:windowExitAnimation">@anim/phone_i_anim_bottom_out_300</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowEnterAnimation">@anim/showup</item>
        <item name="android:windowExitAnimation">@anim/dismiss</item>
    </style>

    <style name="AlertDialogAnimation">
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowExitAnimation">@anim/dismiss</item>
    </style>

    <style name="PopupArchorAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/showup</item>
        <item name="android:windowExitAnimation">@anim/dismiss</item>
    </style>

    <style name="FragmentAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/slide_in_right</item>
        <item name="android:windowExitAnimation">@anim/slide_out_left</item>
        <!-- <item name="android:windowExitAnimation">@animator/shake</item> -->
    </style>

    <style name="DialogSlideAnim">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:alertDialogStyle">@android:style/Theme.Dialog</item>
        <item name="android:windowAnimationStyle">@style/PopupArchorAnimation</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ProgressDialogSlide">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:alertDialogStyle">@android:style/Theme.Dialog</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="DialogAnimationTransparent">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowEnterAnimation">@anim/showup</item>
        <item name="android:windowExitAnimation">@anim/dismiss</item>
        <item name="android:windowBackground">@color/transparent_color</item>
        <item name="android:windowFrame">@color/transparent_color</item>
        <item name="android:windowNoTitle">true</item>
    </style>

